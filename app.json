{
  "expo": {
    "name": "shift-filla-mobile",
    "slug": "shift-filla-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Briefcase_550x550.png",
    "scheme": "shiftfillamobile",
    "userInterfaceStyle": "automatic",
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "com.matthew.ardern93.shiftfillamobile"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/Briefcase_550x550.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "package": "com.matthew.ardern93.shiftfillamobile"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/Briefcase_550x550.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "forceStaticLinking": [
              "RNFBApp",
              "RNFBAuth",
              "RNFBFirestore",
              "RNFBFunctions",
              "RNFBStorage"
            ],
            "useFrameworks": "static",
            "ccacheEnabled": true
          }
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2f2292be-ecd2-429a-82b5-759041e470c9"
      }
    }
  }
}
